<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context.xsd">

    <bean class="org.example.modal.Mytime" id="time1">
        <property name="hours" value="30"></property>
        <property name="minutes" value="30"></property>
        <property name="seconds" value="56"></property>
    </bean>

    <bean class="org.example.modal.Mytime" id="time2">
        <property name="hours" value="10"></property>
        <property name="minutes" value="20"></property>
        <property name="seconds" value="36"></property>
    </bean>

</beans>